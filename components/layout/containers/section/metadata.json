{
  "name": "Section Container",
  "description": "Full-width section wrapper that can contain other components",
  "category": "layout",
  "subcategory": "containers",
  "type": "container",
  "canContain": ["component", "container"],
  "maxChildren": 20,
  "tags": ["container", "section", "wrapper", "layout"],
  "responsive": true,
  "darkMode": true,
  "dependencies": ["tailwindcss"],
  "version": "1.0.0",
  "author": "TailwindUI Blocks",
  "created": "2025-06-19",
  "updated": "2025-06-19",
  "defaultProps": {
    "padding": "md",
    "background": "transparent",
    "spacing": "normal"
  },
  "configurableProps": [
    {
      "name": "padding",
      "type": "select",
      "label": "Padding",
      "options": [
        {"value": "none", "label": "None"},
        {"value": "sm", "label": "Small"},
        {"value": "md", "label": "Medium"},
        {"value": "lg", "label": "Large"},
        {"value": "xl", "label": "Extra Large"}
      ],
      "default": "md"
    },
    {
      "name": "background",
      "type": "select",
      "label": "Background",
      "options": [
        {"value": "transparent", "label": "Transparent"},
        {"value": "white", "label": "White"},
        {"value": "gray-50", "label": "Light Gray"},
        {"value": "gray-900", "label": "Dark Gray"}
      ],
      "default": "transparent"
    },
    {
      "name": "spacing",
      "type": "select",
      "label": "Vertical Spacing",
      "options": [
        {"value": "none", "label": "None"},
        {"value": "tight", "label": "Tight"},
        {"value": "normal", "label": "Normal"},
        {"value": "loose", "label": "Loose"}
      ],
      "default": "normal"
    }
  ],
  "features": [
    "Full-width container",
    "Configurable padding",
    "Multiple background options",
    "Responsive design",
    "Dark mode support",
    "Drag & drop compatible"
  ]
}